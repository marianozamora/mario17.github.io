body 
	text-align center
	margin 0
	padding 0
	
header
	height 72px
	width 100%
	position fixed
	background-color #fff
	border-bottom solid 1px transparent
	z-index 99
	border-bottom-color #e9f0f3
	section
		max-width 1030px
		width 100%
		margin 0 auto
		padding 0 15px
		height 100%
		.Header-activeMenu
			position absolute
			right 20px
			top 20px
			border none
			padding 10px
			border-radius 10px
			display none
			.close
				transform rotate(-90deg)
				-webkit-transform rotate(-90deg)
				-webkit-transition all 900ms ease-in-out
				transition all 900ms ease-in-out
				&:before
					font-family 'Iconos'
					content "\e672"
					font-size 30px
					line-height 13px
					font-style normal
			.open 
				transform rotate(-90deg)
				-webkit-transform rotate(-90deg)
				-webkit-transition all 900ms ease-in-out
				transition all 900ms ease-in-out
				&:before
					font-family 'Iconos'
					content "\e670"
					font-size 30px
					line-height 13px
					font-style normal
			&.is-active
				display block
			    

		.MainMenu
			background white
			width 250px
			height calc(800% + 60px)
			margin 0 
			position absolute 
			left -250px
			-webkit-transition .3s
			top 0
			padding 0
			&.is-active
				left 0
			&-item
				height 50px
				line-height 50px
				list-style none
				text-align center
				padding 0 20px
				&:hover
					background gray
			&-link
				color black
				text-decoration none
				display block
				text-transform uppercase
				font-size 14px
				font-weight bold
		figure
			float left
			margin 0 !important
			padding 0 
			display inline-block
			.litlelogo
				.logoone
					position relative !important
					top 6px !important
					width 44px
					height 50px
					
			.otherlogo
				.logotwo
					position relative
					top 12px
					//margin-top 5px
					width 160px
					height 50px
		nav
			display inline-block
			float right
			margin 10px 0
			color gris_claro
			font-size inherit
			line-height 1.6
			
			ul 
				display inline-block
				li 
					display inline-block
					a 
						display inline-block
						margin-right 20px
						text-decoration none
						color gray
					#lastid
						font-weight bold
						color celeste_claro


							
